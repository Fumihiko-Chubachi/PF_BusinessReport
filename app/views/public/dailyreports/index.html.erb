<div class="container">
  <div class="row">
    <div class="col-2">
      <%= render 'public/works/sidebar' %>
    </div>
  <div class="col-10">
    <h1>業務日報一覧</h1>
    <% @dailyreports.each do |dailyreport| %>
      <div>
        <%= link_to dailyreport.created_at.strftime('%Y/%m/%d'), public_dailyreport_path(dailyreport.id) %>
      </div>
    <% end %>
  </div>
</div>